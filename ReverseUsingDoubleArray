public class ReverseUsingDoubleArray {
    public static void main(String[] args) {
int[] arr = {1, 2, 3, 4, 5};
        int[] arr1 = new int[arr.length];
        int x=0;
        for (int i = arr.length - 1; i >= 0; i--)
        {
            arr1[x]= arr[i];
            x++;


        }
        for(int i=0;i<arr1.length;i++)
        System.out.print(arr1[i]+ " ");
        }
        }
