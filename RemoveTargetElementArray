public class RemoveTargetElementArray {
    public static void main(String[] args) {
        int[] arr = {1,2,4,5,8,6,5,7};
        int n = 5;        
        for(int i=0;i<arr.length;i++) {
            if (arr[i] == n) {
                continue;
            }
            System.out.print(arr[i] + ",");
        }
    }
}
